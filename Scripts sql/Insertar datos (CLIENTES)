  CREATE TABLE Proveedor (
  id_proveedor INT,
  nombre VARCHAR(40),
  direccion VARCHAR(128),
  pais VARCHAR(30),
  ciudad VARCHAR(30),
  telefono VARCHAR(20),
  e_mail VARCHAR(64),
  PRIMARY KEY (id_proveedor));
  
