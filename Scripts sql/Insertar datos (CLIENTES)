CREATE DATABASE AlquilerDeVehiculos;
USE AlquilerDeVehiculos;

CREATE TABLE Cliente (
  id_cliente INT,
  ruc_ci BOOLEAN,
  identificacion VARCHAR(20),
  nombre  VARCHAR(40),
  telefono VARCHAR(11),
  direccion VARCHAR(128),
  razon_social VARCHAR(128),
  PRIMARY KEY (id_cliente));
  
