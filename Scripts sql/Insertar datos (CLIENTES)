CREATE TABLE Departamento (
  id_departamento INT,
  id_supervisor INT,
  nombre  VARCHAR(45),
  PRIMARY KEY (id_departamento),
  FOREIGN KEY (id_supervisor) REFERENCES Empleado(id_empleado));

