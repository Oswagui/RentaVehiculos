CREATE TABLE Empleado (
  id_empleado INT,
  nombre VARCHAR(30),
  apellido VARCHAR(30),
  cedula VARCHAR(10),
  usuario VARCHAR(15),
  contrasena VARCHAR(16),
  sueldo FLOAT,
  fecha_inicio DATETIME,
  puesto VARCHAR(128),
  grupo_trabajo VARCHAR(128),
  hora_entrada TIME,
  hora_salida TIME,
  telefono VARCHAR(11),
  direccion VARCHAR(128),
  PRIMARY KEY (id_empleado));
  
