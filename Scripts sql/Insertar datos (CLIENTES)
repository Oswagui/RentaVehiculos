CREATE TABLE Cuidado (
  empleado INT,
  vehiculo VARCHAR(10),
  observaciones VARCHAR(1024),
  PRIMARY KEY (empleado, vehiculo),
  FOREIGN KEY (empleado) REFERENCES Empleado(id_empleado),
  FOREIGN KEY (vehiculo) REFERENCES Vehiculo(matricula));
  
