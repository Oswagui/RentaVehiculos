CREATE TABLE Vehiculo (
  matricula VARCHAR(10),
  proveedor  INT,
  tipo   VARCHAR(20),
  marca VARCHAR(20),
  año INT,
  nombre_modelo VARCHAR(20),
  disponibilidad BOOLEAN,
  color VARCHAR (20),
  capacidad INT,
  precio  FLOAT,
  foto LONGBLOB,
  PRIMARY KEY (matricula),
  FOREIGN KEY (proveedor) REFERENCES Proveedor(id_proveedor));
  
