CREATE TABLE Reparacion (
  idReparacion INT,
  empleado INT,
  vehiculo VARCHAR(10),
  costo FLOAT,
  fecha_reparación DATETIME,
  descripcion VARCHAR(256),
  PRIMARY KEY (idReparacion),
  FOREIGN KEY (empleado) REFERENCES Empleado(id_empleado),
  FOREIGN KEY (vehiculo) REFERENCES Vehiculo(matricula));
  
